{
  "name": "external-project",
  "author": "freeCodeCamp",
  "description": "Template used for freeCodeCamp projects with the freeCodeCamp Courses VSCode extension",
  "scripts": {
    "start": "node ./tooling/server.js",
    "develop": "npm run develop:client & npm run develop:server",
    "build:git": "node ./tooling/git/build.js",
    "build:client": "NODE_ENV=production webpack",
    "develop:client": "webpack --watch --mode development",
    "develop:server": "nodemon --watch ./dist/ ./tooling/server.js"
  },
  "workspaces": [
    "./client"
  ],
  "dependencies": {
    "chai": "4.3.6",
    "chokidar": "3.5.3",
    "express": "4.18.1",
    "marked": "4.0.15",
    "prismjs": "^1.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ws": "8.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freeCodeCamp/external-project"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@types/marked": "4.0.3",
    "@types/prismjs": "^1.26.0",
    "@types/react": "17.0.45",
    "@types/react-dom": "17.0.16",
    "babel-loader": "8.2.5",
    "babel-plugin-prismjs": "^2.1.0",
    "css-loader": "6.7.1",
    "dotenv-webpack": "^7.1.0",
    "html-webpack-plugin": "5.5.0",
    "nodemon": "2.0.16",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "style-loader": "3.3.1",
    "ts-loader": "9.3.0",
    "typescript": "4.6.4",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "type": "module"
}
